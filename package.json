{
  "name": "repfl",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://repfl.ch",
  "dependencies": {
    "concrete.css": "^1.1.1",
    "normalize.css": "^8.0.1",
    "preact": "^10.0.0-rc.1",
    "reeq": "^1.6.0"
  },
  "scripts": {
    "start": "parcel src/index.html --port 3000 --hmr-port 1234",
    "lint": "standard --fix",
    "predeploy": "rimraf dist && parcel build --public-url ./ src/index.html --experimental-scope-hoisting",
    "deploy": "echo 'repfl.ch' > ./dist/CNAME && gh-pages -d dist"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^2.6.3",
    "standard": "^13.1.0"
  }
}
